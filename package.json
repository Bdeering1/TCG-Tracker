{
  "name": "tcg-tracker",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "./build/index.js",
  "scripts": {
    "precommit": "eslint .",
    "build": "tsc --build --force --dry && esbuild $(esbuild-config)",
    "build-prod": "esbuild $(esbuild-config) --sources-content=false --minify",
    "clean": "rm -rf ./build",
    "dev": "docker compose --profile=dev up -d && concurrently 'esbuild $(esbuild-config) --watch' 'nodemon --es-module-specifier-resolution=node .'",
    "prod": "docker compose --profile=prod up",
    "start": "node --es-module-specifier-resolution=node --stack-trace-limit=15 ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bdeering1/TCG-Tracker.git"
  },
  "keywords": [],
  "author": "Bryn Deering",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "concurrently": "^7.6.0",
    "esbuild": "0.17.10",
    "esbuild-config": "^1.0.1",
    "eslint": "^8.34.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cheerio": "1.0.0-rc.12",
    "express": "^4.18.2",
    "mongoose": "^6.9.2"
  }
}
